     p_codes :
      -
        code : |1+
         PROCEDURE MyWindow()
         
         // Customer
         gsonnectionID is string
      -
        code : |1+
         IF gsonnectionID="" THEN
         	// Disconnected
         	BTN_Connection_to_server..State = Active
         	BTN_Disconnection_from_server..State = Grayed
         	BTN_Ask_for_date_synchronous_call..State = Grayed
         	BTN_Ask_for_time_asynchronous_call..State = Grayed
         ELSE
         	// Connected
         	BTN_Connection_to_server..State = Grayed
         	BTN_Disconnection_from_server..State = Active
         	BTN_Ask_for_date_synchronous_call..State = Active
         	BTN_Ask_for_time_asynchronous_call..State = Active
         END
        type : 234
